Sub skilldrill()

Worksheets("Sheet1").Activate

Dim i As Integer
    For i = 1 To 10
    
Cells(i, 10).Value = 1
Cells(10, i).Value = 1
Cells(i, 9).Value = 1
Cells(9, i).Value = 1
Cells(i, 8).Value = 1
Cells(8, i).Value = 1
Cells(i, 7).Value = 1
Cells(7, i).Value = 1
Cells(i, 6).Value = 1
Cells(6, i).Value = 1
Cells(i, 5).Value = 1
Cells(5, i).Value = 1
Cells(i, 4).Value = 1
Cells(4, i).Value = 1
Cells(i, 3).Value = 1
Cells(3, i).Value = 1
Cells(i, 2).Value = 1
Cells(2, i).Value = 1
Cells(i, 1).Value = 1
Cells(1, i).Value = 1



Next i



End Sub

